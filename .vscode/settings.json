{
  "jupyter.notebookFileRoot": "${workspaceFolder}",
  "files.exclude": {
    "**/env": false,
    "**/src": false,
    "**/.vscode": false,
    "**/public/image": false,
    "**/public/template": false,
    ".gitignore": false,
    "requirements.txt": false,
    "**/__pycache__": true
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true
  }
}